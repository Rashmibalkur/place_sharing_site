<h1>Users</h1>
<table>
  <thead>
    <tr>
      <th>Sr.NO</th>
      <th>User name</th>
      <th>Emil</th>
    </tr>
  </thead>

  <tbody>
    <% @users.each_with_index do |user,index| %>
      <tr>
        <td><%= index +1%></td>
        <td><%= user.user_name%></td>
        <td><%= user.email%></td>
        <td><%= link_to 'View', user_places_path(user_name:user.user_name,id:user.id) %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to 'Shared Places', root_path %>
